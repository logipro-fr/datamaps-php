{
    "name": "logipro/datamaps-php",
    "type": "library",
    "description": "Query Datamaps API",
    "keywords": ["map", "datamaps"],
    "homepage": "https://github.com/logipro-fr/datamaps-php",
    "license": "Copyrighted",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Alban Bernard"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-client": "^7.0"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.27",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "1.10.15",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "DatamapsPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DatamapsPHP\\Tests\\": "tests/unit"
        }
    }
}
